{
  "arrowParens": "always",
  "bracketSpacing": false,
  "endOfLine": "crlf",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "parser": "babel-flow"
      }
    }
  ]
}
